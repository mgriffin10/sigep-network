<%= link_to("<< Main Menu", {:controller => 'access', :action => 'index'}, :class => 'back-link') %>

<h2>Update Profile</h2>
<%= form_for(:profile, 
				:multipart => true, 
				:url => {:action => 'update', :id => @profile.id},
				:html => {:class => 'profile-form'}) do |f| %>
		<%= render(:partial => "form", :locals => {:f => f}) %>
		<%= submit_tag("Update Profile", :class => "btn btn-default") %>
<% end %>
